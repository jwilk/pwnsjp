<refnamediv>
	<refname>&p;</refname>
	<refpurpose>an interactive interface to &d;</refpurpose>
</refnamediv>

<refsynopsisdiv>
	<cmdsynopsis>
		<command>&p;</command>
	</cmdsynopsis>
	<cmdsynopsis>
		<command>&p;</command>
		<group choice='req'>
			<arg choice='plain'><option>--version</option></arg>
			<arg choice='plain'><option>-v</option></arg>
		</group>
	</cmdsynopsis>
	<cmdsynopsis>
		<command>&p;</command>
		<group choice='req'>
			<arg choice='plain'><option>--help</option></arg>
			<arg choice='plain'><option>-h</option></arg>
		</group>
	</cmdsynopsis>
</refsynopsisdiv>
<refsect1>
	<title>DESCRIPTION</title>
	<para>
		&p; runs an interactive interface to the &d;.
	</para>
</refsect1>

<refsect1>
	<title>OPTIONS</title>
	<para>
		&p; accepts the following options:
	</para>

	<variablelist>
		<varlistentry>
			<term><option>-h</option> or <option>--help</option></term>
			<listitem>
				<para>Display help and exit.</para>
			</listitem>
		</varlistentry>
		<varlistentry>
			<term><option>-v</option> or <option>--version</option></term>
			<listitem>
          		<para>Output version information and exit.</para>
			</listitem>
		</varlistentry>
	</variablelist>
</refsect1>

<refsect1>
	<title>FILES</title>
	<variablelist>
		<varlistentry>
			<term>&slowin;</term>
			<listitem>
				<para>The data file of the &d;.</para>
			</listitem>
		</varlistentry>
	</variablelist>
</refsect1>

