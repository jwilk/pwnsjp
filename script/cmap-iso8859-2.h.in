#!/usr/bin/perl
print "char rev_iso88592[0xe0] = \"";

my $s,$null,$iso,$uni;
for($uni=0xa0; $uni<0x17f; $uni++)
  { $s{$uni}=0; }

while(<STDIN>)
{
  if ($_ =~ /^\<U/)
  {
    s/^\<U([0-9A-F]{4})\> *\/(x[0-9a-f]{2}).*/0x\1:0\2/;
    ($uni,$iso) = split ':', $_;
    $iso=eval $iso;
    $uni=eval $uni;
    $s{eval $uni}=$iso;
  }
}
for($uni=0xa0; $uni<0x17f; $uni++)
  { printf "\\x%02x", $s{$uni} };
print "\";\n"

