#!/usr/bin/perl
print "#ifndef CMAP_ISO8859_2_H\n".
      "#define CMAP_ISO8859_2_H\n\n". 
      "char rev_iso88592[0xe0] = \"";

foreach $uni (0xa0 .. 0x17f)
  { $s{$uni}=0; }

while(<STDIN>)
{
  next if ($_ !~ /^\<U/);
  s/^\<U([0-9A-F]{4})\> *\/(x[0-9a-f]{2}).*/0x\1:0\2/;
  ($uni,$iso) = split /:/;
  $iso=eval $iso;
  $uni=eval $uni;
  $s{eval $uni}=$iso;
}
foreach $uni (0xa0 .. 0x17f)
  { printf "\\x%02x", $s{$uni} };
print "\";\n\n#endif\n"

