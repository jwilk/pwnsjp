#!/bin/sh
echo "#ifndef CMAP_USASCII_H"
echo "#define CMAP_USASCII_H"
echo
echo -n 'char* rev_usascii[0xe0] = { "\xa0", '
(
  for i in `seq 161 383`
  do
    echo "&#$i;"
  done 
) |
konwert html-ascii |
  sed -re 's/"/\\"/; s/(.*)$/"\1",/;' | 
  tr "\n" " " |
  sed -re 's/, $//'
echo " };"
echo
echo "#endif"

